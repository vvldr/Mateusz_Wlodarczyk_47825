using System;

namespace kalkulator_silnia_wpisz_wartość
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Podaj Wartość N silnia\n");
            int N = int.Parse(Console.ReadLine());

            int Silnia = 1;


            for (int i = 1; i <= N; i++)
            {
                Silnia = Silnia * i;


            }
            Console.Write(Silnia);
        }
    }
}
