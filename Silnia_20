using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Zadanie2_1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            long  silnia = 1;
            long liczba = 20;

            for (long i = 1; i <= liczba; i++)
            {

                silnia *= i;

            }

            Console.WriteLine(silnia);


            Console.ReadKey();
        }
    }
}
